{
  "name": "testcafe-cucumber",
  "version": "0.1.0",
  "description": "An integration of TestCafe and CucumberJS",
  "author": "CHGovindarao <chgovinda.rao@springernature.com>",
  "contributors": [
    "Rakesh sharma"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    
    "debug": "node --inspect=1337 --debug-brk --nolazy node_modules/cucumber/bin/cucumber-js --tags @debug --format json:./reports/report.json",
    "test1": "./node_modules/cucumber/bin/cucumber-js",
    "test-report": "./node_modules/cucumber/bin/cucumber-js --format json:./reports/report.json",
    "test-chrome": "./node_modules/cucumber/bin/cucumber-js --world-parameters \"{\\\"browser\\\": \\\"chrome\\\"}\"",
    "test-ie": "./node_modules/cucumber/bin/cucumber-js  --world-parameters \"{\\\"browser\\\": \\\"ie\\\"}\"",
    "test-edge": "../node_modules/cucumber/bin/cucumber-js  --world-parameters \"{\\\"browser\\\": \\\"edge\\\"}\"",
    "test-firefox": "./node_modules/cucumber/bin/cucumber-js  --world-parameters \"{\\\"browser\\\": \\\"firefox\\\"}\"",
    "test-opera": "./node_modules/cucumber/bin/cucumber-js --world-parameters \"{\\\"browser\\\": \\\"opera\\\"}\"",
    "test-safari": "./node_modules/cucumber/bin/cucumber-js  --world-parameters \"{\\\"browser\\\": \\\"safari\\\"}\"",
    "test-chrome-report": "./node_modules/cucumber/bin/cucumber-js  --format json:./reports/report.json --world-parameters \"{\\\"browser\\\": \\\"chrome\\\"}\"",
    "test-ie-report": "./node_modules/cucumber/bin/cucumber-js  --format json:./reports/report.json --world-parameters \"{\\\"browser\\\": \\\"ie\\\"}\"",
    "test-edge-report": "./node_modules/cucumber/bin/cucumber-js  --format json:./reports/report.json --world-parameters \"{\\\"browser\\\": \\\"edge\\\"}\"",
    "test-firefox-report": "./node_modules/cucumber/bin/cucumber-js  --format json:./reports/report.json --world-parameters \"{\\\"browser\\\": \\\"firefox\\\"}\"",
    "test-opera-report": "./node_modules/cucumber/bin/cucumber-js  --format json:./reports/report.json --world-parameters \"{\\\"browser\\\": \\\"opera\\\"}\"",
    "test-safari-report": "./node_modules/cucumber/bin/cucumber-js  --format json:./reports/report.json --world-parameters \"{\\\"browser\\\": \\\"safari\\\"}\"",
    "test-chrome-headless": "./node_modules/cucumber/bin/cucumber-js  --world-parameters \"{\\\"browser\\\": \\\"chrome:headless\\\"}\"",
    "test-firefox-headless": "./node_modules/cucumber/bin/cucumber-js  --world-parameters \"{\\\"browser\\\": \\\"firefox:headless\\\"}\""
  },
  "dependencies": {
    "base64-img": "^1.0.4",
    "eslint": "^4.19.1",
    "npm": "^6.0.0",
    "testcafe-reporter-html": "^1.4.6"
  },
  "devDependencies": {
    "cucumber": "^5.1.0",
    "cucumber-html-reporter": "^5.2.0",
    "eslint-config-google": "^0.9.1",
    "gherkin-testcafe": "^2.5.0",
    "testcafe": "^1.8.4",
    "testcafe-browser-provider-browserstack": "^1.13.0-alpha.1",
    "testcafe-cucumber-steps": "^1.8.4"
  }
}
